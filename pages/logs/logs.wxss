.swiper-tab{
	width: 100%;
	text-align: center;
	line-height: 80rpx;
}
.swiper-tab-list{
  font-size: 30rpx;
  display: inline-block;
	width: 33.33%;
	color: #777777;
  
}
.on{ 
  color: #da7c0c;
}
.swiper-box{ 
  display: block; 
  height: 100%; 
  width: 100%; 
  overflow: hidden; 
}
.swiper-box view{
	text-align: center;
}
.log-list {
  display: flex;
  flex-direction: column;
  width:100%;
  height:100%;
}
.design{
  margin-top:5px;
  padding-bottom:5rpx;
  margin-left:2rpx;
  width: 98%;
  height:80rpx;
  line-height:80rpx;
  border-radius:128rpx 0 0 128rpx;
  background-color: #eee;
  font-size:30rpx;
}
.news{
  margin-top:300rpx;
  margin-left:80rpx;
  width: 80%;
  height:200rpx;
  line-height:200rpx;
  border-radius:128rpx 128rpx 128rpx 128rpx;
  background-color: #ffa500;
  font-size:30rpx;
}
.ranks-num{
  width:10%;
  height:80rpx;
  float:left;
}
.imgs{
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
}
.log-name{
  margin-left:20rpx;
  float:left;
  width:35%;
  height:80rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.log-time{
  margin-left:20rpx;
  width:15%;
  height:80rpx;
  float:left;
  color:#bd1c1c;
}
.rank-list{
    width:100%;
    height:100%;
    font-size: 30rpx;
    text-align: center;
}
.rank-box{
  margin-left:30px;
  margin-top:5px;
  padding-bottom:5rpx;
  width: 85%;
  height: 80rpx;
  line-height:80rpx;
  border-radius: 128rpx 0 0 128rpx;
  background-color: #eee;
  font-size:30rpx;
}
.rank-num{
  float:left;
  width:10%;
  height:80rpx;
}
.img-box{
  float:left;
  width:10%;
  height:80rpx;
}
.rank-name{
  margin-left:20rpx;
  float:left;
  width:40%;
  height:80rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;

}
.rank-points{
  float:left;
  width:35%;
  height:80rpx;
  color:#bd1c1c;
}
.images{
  width:85rpx;
  height:85rpx;
}